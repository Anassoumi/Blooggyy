
<%- include("partials/header"); -%>
    <h1>Home</h1>
    <p> <%= startingContent %> </p>


    <div class="layout">

    <%  posts.forEach((post)=>{ %>

      <div class="back">
      <h1> <%= post.name %></h1>
      <p><%= post.Text.substring(0, 100) + " ..." %>
        <a href="/posts/<%=post.name.replace(/ /g,'')%>">Read More</a></p>
      </div>

      <% }) %>

    </div>

 


<%- include("partials/footer"); -%>
